

@{
    ViewData["Title"] = "Index";
}
@model List<EventDataDto>
<a class="btn btn-primary" asp-controller="EventDatas" asp-action="Save">Save Event</a>
    <table class="table table-striped">
        <thead>
            <tr>
            <td>Id</td>
            <td>EventTime</td>
            <td>EventType</td>
            <td>TerminalId</td>
            <td>UserRecNo</td>
            
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
               <td>@item.Id</td>
                <td>@item.EventTime</td>
                 <td>@item.EventType</td>
                  <td>@item.TerminalId</td>
                   <td>@item.UserRecNo</td>
                    <td>
                        <a class="btn btn-warning" asp-controller="EventDatas" asp-action="Update" asp-route-id="@item.Id">Update</a>
                          <a class="btn btn-danger" asp-controller="EventDatas" asp-action="Remove" asp-route-id="@item.Id">Delete</a>
                        </td>
                     </tr>
            }

        </tbody>



    </table>


