
@{
    ViewData["Title"] = "Save";
}
@model EventDataDto
<h1>Save Event</h1>

<div class="row">

    <div  class=col-md-6>

        <form asp-action="Save" method="post" asp-controller="EventDatas">

            <div class="form-group">
                <label asp-for="EventTime"></label>
                <input asp-for="EventTime" class="form-control" />
                <span asp-validation-for="EventTime" class="text-danger"></span>
            </div>
             <div class="form-group">
                <label asp-for="EventType"></label>
                <input asp-for="EventType" class="form-control" />
                <span asp-validation-for="EventType" class="text-danger"></span>
            </div>

              <div class="form-group">
                <label asp-for="TerminalId"></label>
                <input asp-for="TerminalId" class="form-control" />
                <span asp-validation-for="TerminalId" class="text-danger"></span>
            </div>
             <div class="form-group">
                <label asp-for="UserRecNo"></label>
                <input asp-for="UserRecNo" class="form-control" />
                <span asp-validation-for="UserRecNo" class="text-danger"></span>
            </div>
              

            <div class="form-group">

                <input type="submit" value="Save" class="btn-primary" />

            </div>
        </form>



    </div>


</div>

